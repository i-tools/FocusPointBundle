{
    "name": "paktdigital/focus-point-bundle",
    "description": "Symfony bundle for third774's image focus, for image cropping based on a focus point.",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Hugo Hulsebosch",
            "email": "hugo.hulsebosch@wearejust.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "liip/imagine-bundle": "^2.0",
        "symfony/asset": "^5.0",
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/form": "^5.0",
        "symfony/options-resolver": "^5.0",
        "vich/uploader-bundle": "^1.16"
    },
    "suggest": {
        "symfony/twig-bundle": "Allows usage of the focus point Twig extension."
    },
    "autoload": {
        "psr-4": {
            "PaktDigital\\FocusPointBundle\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "0.12.*"
    },
    "scripts": {
        "fix-cs": "php vendor/bin/php-cs-fixer fix --diff",
        "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon src --level 7"
    }
}
